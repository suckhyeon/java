package test240802.object1;

import java.net.InetAddress;
import java.net.Socket;

public class TCPClient {
		public void client() {
		
		int port = 3000;
		String serverIP;
		
		try {
			serverIP = InetAddress.getLocalHost().getHostAddress();
			
			Socket socket = new Socket(serverIP, port);
			}		
		}
}
